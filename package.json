{
    "name": "hidb-back",
    "version": "1.0.0",
    "description": "backend for plans history db",
    "main": "server.ts",
    "scripts": {
        "dev": "nodemon --watch src --exec ts-node src/server.ts",
        "build": "tsc",
        "start": "ts-node src/server.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:unit": "jest --testPathIgnorePatterns=integration",
        "test:integration": "RUN_INTEGRATION_TESTS=true jest --testPathPattern=integration",
        "test:verbose": "jest --verbose",
        "test:silent": "jest --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/haagahelia/hidb-back.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/haagahelia/hidb-back/issues"
    },
    "homepage": "https://github.com/haagahelia/hidb-back#readme",
    "dependencies": {
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "knex": "^3.1.0",
        "mysql2": "^3.15.3"
    },
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/express-validator": "^3.0.2",
        "@types/knex": "^0.15.2",
        "@types/node": "^24.9.1",
        "@types/supertest": "^6.0.3",
        "jest": "^30.2.0",
        "nodemon": "^3.1.10",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
}
